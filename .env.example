# Database (Supabase Postgres)
DATABASE_URL=postgresql://postgres:password@host:5432/postgres

# APIs
TYPEFULLY_API_KEY=replace_me
SEMRUSH_API_KEY=replace_me
APIFY_TOKEN=replace_me

# GA4 Service Account JSON, base64-encoded
GA4_JSON_KEY_B64=replace_with_base64_json
GA4_PROPERTY_ID=

# Brandwatch (optional)
BRANDWATCH_USERNAME=
BRANDWATCH_PASSWORD=
# or
# BRANDWATCH_API_TOKEN=

# Optional tuning
HTTP_TIMEOUT_SECONDS=30
HTTP_MAX_RETRIES=3
BACKOFF_BASE_SECONDS=1.0